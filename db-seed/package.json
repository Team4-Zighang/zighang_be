{
  "name": "init-db",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:seed": "tsx ./seed.ts",
    "db:push": "drizzle-kit push --config ./drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config ./drizzle.config.ts",
    "db:generate": "drizzle-kit generate --config ./drizzle.config.ts",
    "db:drop": "tsx scripts/drop-db.ts",
    "db:clear": "tsx scripts/clear-db.ts",
    "db:reset": "npm run db:drop && npm run db:push",
    "db:migrate-manual": "npm run generate && tsx scripts/manual-migrate.ts",
    "db:fresh": "npm run db:clear && npm run db:seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@faker-js/faker": "^10.0.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "mysql2": "^3.15.0"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "dotenv": "^17.2.2",
    "tsx": "^4.20.5"
  }
}
